{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-use-before-define": 0,
    "import/no-cycle": 0,
    "prettier/prettier": "error"
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"]
}
